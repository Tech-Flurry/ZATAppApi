@model ZATAppApi.ViewModels.RegisterSubAdminViewModel

@{
    ViewBag.Title = "Registeration Confirmation";
}

<div class="general">
    <div class="panel panel-success">
        <div class="panel-heading">
            <h2 class="title2">@ViewBag.Title</h2>
        </div>
        <div class="panel-body">
            <dl class="dl-horizontal">
                <dt>
                    Name:
                </dt>
                <dd>
                    @Model.FirstName @Model.LastName
                </dd>
                <dt>
                    Username:
                </dt>
                <dd>
                    @Model.Username
                </dd>
                <dt>
                    Password:
                </dt>
                <dd>
                    @ZATAppApi.Common.Constants.DEFAULT_PASSWORD
                </dd>
            </dl>
                 
                 
                 
        </div>
    </div>
</div>